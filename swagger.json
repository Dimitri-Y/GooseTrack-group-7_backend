              "format": "email"
            },
            "password": {
              "type": "string",
              "description": "User's password",
              "example": "qwerty123"
            },
            "userName": {
              "type": "number",
              "description": "User's name",
              "example": "Maks_Marzinkevich"
            }
          }
        },
        "example": [
          {
            "email": "admin@gmail.com",
            "userName": "admin",
            "password": "qwerty123"
          },
          {
            "email": "Natalia@gmail.com",
            "userName": "Natalia",
            "password": "qwerty12334r4r4"
          }
        ]
      },
      "securitySchemes": {
        "Bearer": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    }
